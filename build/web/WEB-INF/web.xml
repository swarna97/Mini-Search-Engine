<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Google</servlet-name>
        <servlet-class>google.Google</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>History</servlet-name>
        <servlet-class>google.History</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>clearHistory</servlet-name>
        <servlet-class>google.clearHistory</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Google</servlet-name>
        <url-pattern>/Google</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>History</servlet-name>
        <url-pattern>/History</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>clearHistory</servlet-name>
        <url-pattern>/clearHistory</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
